// Edit Survey Modal
#editRegulatorQuestion.modal.fade(tabindex='-1', role='dialog', aria-labelledby='editRegulatorQuestionLabel', aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h5#editRegulatorQuestionLabel.modal-title Regulator Question bearbeiten
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') Ã—
      .modal-body
        .container
          form.form-horizontal(role='form', method='POST', action='/register')
            .row
              .col-md-3.field-label-responsive
                label(for='id') ID
              .col-md-9
                .form-group
                  p#id.form-control-static #10
                  
            .row
              .col-md-3.field-label-responsive
